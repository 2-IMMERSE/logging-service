{
  "id": "/edge/logging-service",
  "cmd": null,
  "cpus": 0.25,
  "mem": 128,
  "disk": 0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "gitlab-ext.irt.de:3443/2-immerse/logging-service:latest",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp",
          "labels": {}
        }
      ],
      "privileged": false,
      "parameters": [],
      "forcePullImage": true
    }
  },
  "healthChecks": [
    {
      "path": "/",
      "protocol": "HTTP",
      "portIndex": 0,
      "gracePeriodSeconds": 300,
      "intervalSeconds": 10,
      "timeoutSeconds": 2,
      "maxConsecutiveFailures": 3,
      "ignoreHttp1xx": false
    }
  ],
  "labels": {
    "traefik.frontend.rule": "Host:logging-service-edge.platform.2immerse.eu"
  },
  "uris": [
    "https://origin.platform.2immerse.eu/docker.tar.gz"
  ]
}
